// Place your settings in this file to overwrite the default settings
{
  "terminal.integrated.fontSize": 13,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.exclude": {
    "**/.@__thumb": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": true
  },
  "editor.autoIndent": "full",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnType": false,
  "editor.fontSize": 13,
  "editor.wordWrap": "off",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    120
  ],
  "prettier.singleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "npm.runInTerminal": false,
  "typescript.validate.enable": false,
  "team.showWelcomeMessage": false,
  "window.zoomLevel": 0,
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "php",
    "plaintext",
    "python",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.userWords": [
    "Bing",
    "Bitbounce",
    "Boxbe",
    "Customizers",
    "Gmail",
    "Injectable",
    "Kajabi",
    "MSAL",
    "PNPM",
    "Performant",
    "Pluralsight",
    "Sideload",
    "Signup",
    "Truthy",
    "Voitanos",
    "Wisitia",
    "Wista",
    "Wistia",
    "bot's",
    "customizations",
    "customizer",
    "deserialize",
    "deserialized",
    "deserializer",
    "endregion",
    "instrumeter",
    "lastmod",
    "lcov",
    "lifecycle",
    "odata",
    "pageview",
    "passthrough",
    "preconfigured",
    "prepended",
    "redistributable",
    "routable",
    "screencast",
    "serverless",
    "sharepoint",
    "sideloaded",
    "toolchain",
    "transclude",
    "webhook",
    "webhooks",
    "webpart",
    "xtest"
  ],
  "csharp.suppressDotnetInstallWarning": true,
  "csharp.suppressDotnetRestoreNotification": true,
  "workbench.startupEditor": "none",
  "git.autofetch": true,
  "gitProjectManager.baseProjectsFolders": [
    "/Users/ac/repos"
  ],
  "git.enableSmartCommit": true,
  "explorer.confirmDragAndDrop": false,
  "breadcrumbs.enabled": true,
  "prettier.arrowParens": "always",
  "rest-client.fontSize": 12,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.suggest.autoImports": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.rendererType": "dom",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "markdownlint.customRules": [
    "{docsmsft.docs-linting}/markdownlint-custom-rules/rules.js"
  ],
  "markdownlint.config": {
    "MD025": {
      "front_matter_title": ""
    },
    "MD028": false
  },
  "yaml.schemas": {
    "file:///toc.schema.json": "/toc\\.yml/i"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "insertDateString.format": "YYYY-MM-DDThh:mm:ssZZZ",
  "docs.templates.githubid": "andrewconnell",
  "docs.templates.alias": "v-johnco",
  "docs.templates.learn_repo_id": "learn-m365",
  "explorer.confirmDelete": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "wallaby.startAutomatically": false,
  "editor.largeFileOptimizations": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.integrated.letterSpacing": 1,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontFamily": "Meslo LG M DZ for Powerline",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontFamily": "'JetBrains Mono', Menlo, Monaco, 'Courier New', monospace",
  "cSpell.enableFiletypes": [
    "!c",
    "!cpp",
    "!latex",
    "!python",
    "liquid",
    "yaml"
  ],
  "cSpell.language": "en,en-GB",
  "docs-build.userType": "Public Contributor",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "mssql.connections": [
    {
      "server": "{{put-server-name-here}}",
      "database": "{{put-database-name-here}}",
      "user": "{{put-username-here}}",
      "password": "{{put-password-here}}"
    }
  ],
  "workbench.colorTheme": "Night Owl",
  "workbench.iconTheme": "vscode-icons"
}